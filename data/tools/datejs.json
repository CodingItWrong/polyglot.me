{
	"id": "datejs",
	"name": "Datejs",
	"api": "http://www.datejs.com/",
	"featuresets": {
		"dates": {
			"format": {
				"code": "mydate.toString(myformat)",
				"api": "https://code.google.com/p/datejs/wiki/APIDocumentation#toString"
			},
			"parse": {
				"code": "Date.parse(datestring)",
				"api": "https://code.google.com/p/datejs/wiki/APIDocumentation#parse"
			},
			"compare": {
				"code": [
					"a.equals(b)",
					"a.isBefore(b)",
					"a.isAfter(b)",
					"a.compareTo(b)"
				],
				"api": "https://code.google.com/p/datejs/wiki/APIDocumentation#compareTo"
			},
			"offset": {
				"code": "mydate.add({ days: numdays, months: nummonths })",
				"api": "https://code.google.com/p/datejs/wiki/APIDocumentation#add"
			}
		}
	}
}
